/* components/topbar/index.wxss */

.custom{
    width: 100%;
    background:#fff;
    z-index: 999;
    box-sizing: border-box;
    padding: 0 30rpx;
    position: fixed;
    top: 0;
  }
  .custom .topbar{
    display: flex;
    color: #000;
    font-size: 34rpx;
    font-weight: 500;
    align-items: center;
  }

  image {
    width: 34rpx;
    height: 34rpx;
  }

  .title {
      width: auto;
      margin-left: 10rpx;
      height: 34rpx;
      line-height: 34rpx;
  }