  background: #fff;
  padding: 20rpx 40rpx;
  font-size: 32rpx;
  position: relative;
  display: flex;
  justify-content: start;
}
.menberName_title{
  margin-right: 40rpx;
}

.menberName_delete{
  position: absolute;
  right: 20rpx;
  bottom: 20rpx;
}

.formTwo{
  margin-top: 10rpx;
}

.commitBar{
  display: flex;
  justify-content: flex-end;
}

.apply{
  margin-top: 40rpx;
