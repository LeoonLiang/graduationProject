.thePhone{
  position: fixed;
  bottom: 100rpx;
  right: 40rpx;
}
.thePhone text{
  font-size: 90rpx;
  color: #0081FF;
}

.InfoForm{
  padding: 20rpx 40rpx;
  background: #fff;
  margin-top: 40rpx;
}
.item{
  margin-bottom: 40rpx;
  padding: 20rpx 0;
  border-bottom: 1px #0081FF solid;
  color: #898989;
}

.item input{
  margin-top: 20rpx;
}

.tips{
  margin-top: 20rpx;
  font-size: 36rpx;
  background: #fff;
  padding: 20rpx 40rpx;
}

.menberName{
